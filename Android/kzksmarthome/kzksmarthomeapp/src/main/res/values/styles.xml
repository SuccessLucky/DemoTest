<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 公共 -->
    <style name="back_item_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="Theme.NoTitle.Transparent" parent="@android:style/Theme.NoTitleBar">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="Main" parent="Theme.NoTitle.Transparent">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@color/black</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <!--<item name="android:windowTranslucentStatus">true</item>-->
        <item name="android:fitsSystemWindows">true</item>
    </style>

    <style name="Theme.NoTitle.Gray" parent="Theme.NoTitle.Transparent">
        <item name="android:windowBackground">@color/activity_bg_gray</item>
    </style>

    <style name="Theme.NoTitle.Transparent.Fullscreen" parent="Theme.NoTitle.Transparent">
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="Splash" parent="@android:style/Theme.NoTitleBar.Fullscreen">
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="TopNavSub" parent="Theme.NoTitle.Transparent">
        <item name="android:windowAnimationStyle">@style/FeelyouWindowAnimTheme</item>
        <item name="android:windowIsTranslucent">true</item>

        <item name="android:windowBackground">@color/black</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:fitsSystemWindows">true</item>
    </style>

    <style name="PhotoViewer" parent="Theme.NoTitle.Gray">
        <item name="android:windowAnimationStyle">@style/FadeAnimTheme</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/black</item>
    </style>

    <style name="PhotoViewer.Fullscreen" parent="PhotoViewer">
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="FeelyouWindowAnimTheme" parent="@android:style/Animation.Translucent">
        <item name="android:windowEnterAnimation">@anim/right_side_in</item>
        <item name="android:windowExitAnimation">@anim/right_side_out</item>
    </style>

    <style name="FadeAnimTheme" parent="@android:style/Animation.Translucent">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>

    <style name="bg_layout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <!-- <item name="android:background">@color/activity_bg_gray</item> -->
        <item name="android:orientation">vertical</item>
    </style>

    <style name="back_item_img_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/back_btn</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="back_item_tv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/textsize_16sp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:gravity">center</item>
        <item name="android:drawableLeft">@drawable/all_icon_back_un</item>
        <item name="android:padding">@dimen/topbar_left_with</item>
        <item name="android:textColor">@color/button_text_color_selector</item>
    </style>
    <!-- 首页 -->
    <style name="titlebar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
        <item name="android:background">@color/color_5874a0</item>
    </style>

    <style name="main_RadioButton_style" parent="@style/main_Red_RadioButton_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="main_Red_RadioButton_style">
